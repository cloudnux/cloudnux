{
    "$schema": "../../../node_modules/@cloudnux/cli/dist/schema/entrypoint.schema.json",
    "entries": {
        "get-me": {
            "handler": "getMe",
            "trigger": {
                "type": "http",
                "options": {
                    "method": "GET",
                    "route": "/v1/me"
                }
            }
        },
        "set-me": {
            "handler": "setMe",
            "trigger": {
                "type": "http",
                "options": {
                    "method": "POST",
                    "route": "/v1/me"
                }
            }
        },
        "run-schedule": {
            "handler": "runSchedule",
            "trigger": {
                "type": "schedule",
                "options": {
                    "name": "run-schedule",
                    "pattern": "rate(1 minute)"
                }
            }
        },
        "event-trigger": {
            "handler": "runEvent",
            "trigger": {
                "type": "event",
                "options": {
                    "sourceType": "sqs",
                    "source": "event-trigger-queue"
                }
            }
        }
    }
}