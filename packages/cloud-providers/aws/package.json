{
  "name": "@cloudnux/aws-cloud-provider",
  "description": "CloudNux AWS cloud provider implementation",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudnux/cloudnux.git",
    "directory": "packages/cloud-providers/aws"
  },
  "keywords": [
    "cloud",
    "aws",
    "amazon",
    "ec2",
    "s3",
    "lambda",
    "cloudformation",
    "provider"
  ],
  "scripts": {
    "link": "yarn link",
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@aws-sdk/client-location": "^3.670.0",
    "@aws-sdk/client-s3": "^3.552.0",
    "@aws-sdk/client-sns": "^3.817.0",
    "@aws-sdk/client-sqs": "^3.817.0",
    "@cloudnux/core-cloud-provider": "0.0.1",
    "@cloudnux/utils": "0.0.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.114"
  },
  "peerDependencies": {
    "@cloudnux/core-cloud-provider": "0.0.1",
    "@cloudnux/utils": "0.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}