{
  "name": "@cloudnux/aws-cloud-provider",
  "description": "CloudNux AWS cloud provider implementation",
  "version": "0.4.0",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./router": {
      "types": "./dist/router/index.d.ts",
      "import": "./dist/router/index.mjs",
      "require": "./dist/router/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudnux/cloudnux.git",
    "directory": "packages/cloud-providers/aws"
  },
  "keywords": [
    "cloud",
    "aws",
    "amazon",
    "ec2",
    "s3",
    "lambda",
    "cloudformation",
    "provider"
  ],
  "scripts": {
    "link": "yarn link",
    "build": "tsup",
    "yalc:publish": "yalc publish --private",
    "yalc:push": "yalc push --private",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@aws-sdk/client-location": "^3.670.0",
    "@aws-sdk/client-s3": "^3.552.0",
    "@aws-sdk/client-sns": "^3.817.0",
    "@aws-sdk/client-sqs": "^3.817.0",
    "@cloudnux/core-cloud-provider": "0.1.0",
    "aws-lambda": "^1.0.7"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.52.8",
    "@types/aws-lambda": "^8.10.114"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}