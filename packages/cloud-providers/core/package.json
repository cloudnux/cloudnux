{
  "name": "@cloudnux/core-cloud-provider",
  "version": "0.3.0",
  "description": "CloudNux core interfaces and types for multi-cloud abstraction",
  "license": "MIT",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudnux/cloudnux.git",
    "directory": "packages/cloud-providers/core"
  },
  "keywords": [
    "cloud",
    "multi-cloud",
    "aws",
    "azure",
    "gcp",
    "interfaces",
    "typescript"
  ],
  "scripts": {
    "build": "tsup",
    "yalc:publish": "yalc publish --private",
    "yalc:push": "yalc push --private",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {},
  "devDependencies": {},
  "publishConfig": {
    "access": "public"
